<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tixs.tixsparents">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_parents"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_parents"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegistroActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".CreateProfileActivity" />
        <activity android:name=".CadastraCriancaActivity" />
        <activity android:name=".CancelarIdaActivity" />
        <activity android:name=".BuscaVanActivity"></activity>
    </application>

</manifest>