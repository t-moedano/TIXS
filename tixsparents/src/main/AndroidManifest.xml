<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tixs.tixsparents">

    <uses-permission android:name="android.permission.INTERNET" />

    <meta-data
        android:name="com.google.android.gms.version"
        android:value="@integer/google_play_services_version" />
    <meta-data
        android:name="com.google.android.geo.API_KEY"
        android:value="AIzaSyCJvmVHZm7-ZGP5RSRUJMdHA0RAAmOoaS0" />

    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the 'MyLocation' functionality. 
    -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_parents"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_parents"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegistroActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".CreateProfileActivity" />
        <activity android:name=".CadastraCriancaActivity" />
        <activity android:name=".CancelarIdaActivity" />
        <activity android:name=".BuscaVanActivity" />


        <activity
            android:name=".MostrarLocalizacao"
            android:label="@string/title_activity_mostrar_localizacao"></activity>

        <activity
            android:name=".CancelarVoltaActivity"
            android:label="@string/title_activity_cancelar_volta"
            android:theme="@style/AppTheme.NoActionBar"></activity>

    </application>

</manifest>